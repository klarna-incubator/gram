    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^6.1.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@sentry/react": "^7.6.0",
    "@sentry/tracing": "^7.6.0",
    "dompurify": "^3.1.6",
    "jwt-decode": "^3.1.2",
    "konva": "^8.4.2",
    "lodash": "^4.17.21",
    "marked": "^14.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-konva": "^17.0.2-4",
    "react-konva-utils": "^0.2.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.4.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "use-image": "^1.0.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.4.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "jest-canvas-mock": "^2.4.0",
    "jest-each": "^27.0.6",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^17.0.1",
    "redux-test-utils": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "start": "PORT=4726 REACT_APP_VERSION=$npm_package_version react-scripts start",
    "lint": "prettier --check src/",
    "lint-fix": "prettier -l src/ --write",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "jest --silent",
    "jest": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^react-konva-utils": "<rootDir>/../node_modules/react-konva-utils/es",
      "canvas": "jest-canvas-mock"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!konva|react-konva-utils)"
    ],
    "testEnvironment": "jsdom",
    "setupFiles": [
      "jest-canvas-mock"
    ]
  }
}
