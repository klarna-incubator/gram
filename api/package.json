{
  "name": "@gram/api",
  "private": true,
  "type": "module",
  "version": "4.17.0",
  "description": "API for threat modelling",
  "scripts": {
    "start": "NODE_ENV=development node dist/index.js",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf ./dist",
    "lint": "prettier --check src/",
    "lint-fix": "prettier -l src/ --write",
    "debug": "NODE_ENV=development node --loader ts-node/esm --inspect=127.0.0.1 src/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --detectOpenHandles --no-cache --runInBand --logHeapUsage --forceExit",
    "test-validate": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --detectOpenHandles --no-cache ./src/resources/gram/v1/validation/validateModel.spec.ts"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klarna/gram.git"
  },
  "keywords": [
    "threat model",
    "appsec"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/klarna/gram/issues"
  },
  "homepage": "https://github.com/klarna/gram#readme",
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@gram/config": "^4.17.0",
    "@gram/core": "^4.17.0",
    "@sentry/integrations": "^7.6.0",
    "@sentry/node": "^7.6.0",
    "@sentry/tracing": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.19.2",
    "express-async-error-wrapper": "^1.0.4",
    "express-physical": "^2.0.0",
    "express-prom-bundle": "^6.4.1",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.6.1",
    "tslib": "^2.6.2",
    "ws": "^8.17.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.177",
    "@types/node-cron": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "prettier": "^2.3.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  }
}
